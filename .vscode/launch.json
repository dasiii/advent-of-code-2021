{
    "version": "0.2.0",
    "configurations": [
      {
        "command": "npm run start",
        "name": "start",
        "request": "launch",
        "type": "node-terminal"
      },
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Program",
        "program": "${workspaceFolder}/src/index.ts",
        "preLaunchTask": "tsc: build - tsconfig.json",
        "outFiles": ["${workspaceFolder}/out/**/*.js"]
      }
    ]
  }